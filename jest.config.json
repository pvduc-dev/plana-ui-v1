{
  "testEnvironment": "jest-environment-jsdom",
  "setupFilesAfterEnv": [
    "<rootDir>/setupTest.ts"
  ],
  "transform": {
    "\\.(ts|tsx)$": "babel-jest"
  },
  "collectCoverage": false,
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx}",
    "!src/**/*.stories.tsx",
    "!src/**/*.d.ts",
    "!src/index.ts",
    "!src/components/**/index.ts"
  ],
  "moduleNameMapper": {
    "\\.css$": "identity-obj-proxy"
  }
}
